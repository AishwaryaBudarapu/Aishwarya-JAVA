import java.io.*;
import java.util.*;

public class Solution
{
    public static void main(String[] args) 
    {
      int x1,x2,x3,y1,y2,y3;
      Scanner s=new Scanner(System.in);
        x1=s.nextInt();
        y1=s.nextInt();
        x2=s.nextInt();
        y2=s.nextInt();
        x3=s.nextInt();
        y3=s.nextInt();
        float m1=(float)(x1+x2+x3)/3;
        float m2=(float)(y1+y2+y3)/3;
        System.out.printf("%.1f\n%.1f\n",m1,m2);
    }
}
