import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args) 
    {
        Scanner s=new Scanner(System.in);
        int n,m,s1=0;
        n=s.nextInt();
        m=s.nextInt();
        
        for(int i=n;i<=m;i++)
         {
            s1=s1+i;  
         }
         System.out.printf("%d",s1);
    }
}
