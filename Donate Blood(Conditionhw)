mport java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
    int a;
    float w,h;
    a=sc.nextInt();
    w=sc.nextFloat();
    h=sc.nextFloat();
    if (a>=18)
        { 
         if(w>=45)
        {  
        if(h>=5.5)
    {
     System.out.printf("He or She can donate blood");
       }
      }
    }
    else
    {
     System.out.printf("He or She unable to donate blood");
    }
    }
}
