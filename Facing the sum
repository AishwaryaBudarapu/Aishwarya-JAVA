import java.util.*;
class Test
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
         int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
        }
        int cnt=1;
        int large=a[0];
        for(int i=0;i<n;i++)
        {
             if(large>a[i])
             {
                 cnt++;
             }
        }
              System.out.print(cnt);
     }
}
