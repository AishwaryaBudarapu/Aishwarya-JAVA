import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args) 
    {
        int d,s1=0,t,r=0;
      Scanner s=new Scanner(System.in);
      int n=s.nextInt(); 
       t=n;
       while(n>0)
    {
        d=n%10;
        n=n/10;
        s1+=(d*d*d);
        r+=(d*d*d*d);
    }
    if(s1==t || r==t)
        System.out.println("Yes");
    else
     System.out.println("No");
    }
}
