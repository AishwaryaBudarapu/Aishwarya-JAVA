import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args) 
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int s1=0;
        while(n>0)
        {
          int r=n%10;
          if(r%2==1)
          {
              s1=s1+r;
          }
            n=n/10;
          }
         System.out.println(s1);
        }
    }
