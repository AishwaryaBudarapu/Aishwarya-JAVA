import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args) 
    {
        Scanner s=new Scanner(System.in);
    int a=7;
    int  b=5;
    int n=s.nextInt();
    System.out.print(a+" ");
    System.out.print(b+" ");
    for(int i=3;i<=n;i++)
    {
      if(i%2==1)
     {
          a=a+1;
          System.out.print(a+" ");
     }
        else
        {
            b=b+1;
          System.out.print(b+" ");
       }
    }
    }
}
